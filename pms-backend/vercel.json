{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/rooms/(.*)",
      "dest": "/rooms/$1"
    },
    {
      "src": "/agents/(.*)",
      "dest": "/agents/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}